      <description>&lt;style lang=&#34;css&#34;&gt;&#xA;  @-webkit-keyframes light {&#xA;    from {&#xA;      opacity: 1;&#xA;    }&#xA;&#xA;    20% {&#xA;      opacity: 0.9;&#xA;    }&#xA;&#xA;    40% {&#xA;      opacity: 0.8;&#xA;    }&#xA;&#xA;    60% {&#xA;      opacity: 0.7;&#xA;    }&#xA;&#xA;    80% {&#xA;      opacity: 0.8;&#xA;    }&#xA;&#xA;    to {&#xA;      opacity: 0.9;&#xA;    }&#xA;  }&#xA;&#xA;  @keyframes snow {&#xA;    0% {&#xA;      background-position: 0px 0px, 0px 0px, 0px 0px;&#xA;    }&#xA;&#xA;    100% {&#xA;      background-position: 500px 1000px, 400px 400px, 300px 300px;&#xA;    }&#xA;  }&#xA;&#xA;  body {&#xA;    margin: 0;&#xA;    padding: 0;&#xA;  }&#xA;&#xA;  .header {&#xA;    position: absolute;&#xA;    height: 700px;&#xA;    top: 0;&#xA;    right: 0;&#xA;    left: 0;&#xA;    overflow-x: hidden;&#xA;  }&#xA;&#xA;  .header-back {&#xA;    position: relative;&#xA;    width: 100%;&#xA;    height: 500px;&#xA;    background-color: #899C8C;&#xA;    background-image:&#xA;      url(&#39;https://mploigt.github.io/freiheit//img/xmas/snow1.png&#39;),&#xA;      url(&#39;https://mploigt.github.io/freiheit//img/xmas/snow2.png&#39;),&#xA;      url(&#39;https://mploigt.github.io/freiheit//img/xmas/snow3.png&#39;);&#xA;    animation: snow 25s linear infinite;&#xA;  }&#xA;&#xA;  .header-text {&#xA;    position: absolute;&#xA;    top: 100px;&#xA;    right: 0;&#xA;    left: 0;&#xA;  }&#xA;  &#xA;  .header-text .glow {&#xA;    text-shadow: 1px 1px 10px rgba(255, 255, 255, 0.5);&#xA;  }&#xA;&#xA;  .light {&#xA;    animation: light 3s linear infinite;&#xA;    width: 80dvw;&#xA;    max-width: 600px;&#xA;    position: absolute;&#xA;    left: -130px;&#xA;    top: -50px;&#xA;  }&#xA;&#xA;  .tree {&#xA;    height: 100%;&#xA;    object-fit: contain;&#xA;    position: absolute;&#xA;    right: -300px;&#xA;    bottom: 0px;&#xA;    top: 50px;    &#xA;  }&#xA;  &#xA;  .floor {&#xA;    width: 100%;&#xA;    position: absolute;&#xA;    top: 510px;&#xA;    left: 0;&#xA;   